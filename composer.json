{
    "name": "silverstripe/sitewidecontent-report",
    "description": "Report of all pages and files across all subsites",
    "type": "silverstripe-vendormodule",
    "license": "BSD-3-Clause",
    "keywords": [
        "silverstripe",
        "reports"
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "silverstripe/cms": "^4.0",
        "silverstripe/reports": "^4.11"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.0",
        "silverstripe/subsites": "^2",
        "silverstripe/taxonomy": "^2",
        "silverstripe/contentreview": "^4"
    },
    "conflict": {
        "silverstripe/reports": "<4.11.0"
    },
    "authors": [
        {
            "name": "Carlos Barberis",
            "email": "carlos@silverstripe.com"
        },
        {
            "name": "Christopher Pitt",
            "email": "cgpitt@gmail.com"
        }
    ],
    "extra": {
        "expose": [
            "javascript",
            "css"
        ]
    },
    "autoload": {
        "psr-4": {
            "SilverStripe\\SiteWideContentReport\\": "src/",
            "SilverStripe\\SiteWideContentReport\\Tests\\": "tests/",
            "SilverStripe\\SiteWideContentReport\\Tests\\Behat\\Context\\": "tests/behat/src/"
        }
    },
    "suggest": {
        "silverstripe/subsites": "In order to report across subsites, the subsites module must be installed"
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}